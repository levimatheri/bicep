{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentParameters.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "location": {
      "value": "eastus"
    },
    "appName": {
      "expression": "[json(externalInputs('ev2_scopeBinding_0'))]"
    }
  },
  "externalInputDefinitions": {
    "ev2_scopeBinding_0": {
      "kind": "ev2.scopeBinding",
      "config": "BINDING"
    }
  }
}